{
    "manifest_version": 3,
    "name": "HTML Audio Tag Invoker",
    "version": "1.0",
    "description": "Invokes certain attributes from the audio tag",
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ]
}